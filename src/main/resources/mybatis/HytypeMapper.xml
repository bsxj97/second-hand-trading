<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.daowen.mapper.HytypeMapper">


      <select id="getEntityPlus"   resultType="hytype">
                 select  * from  hytype
                 where  1=1
      </select>
      
       <select id="loadPlus"   resultType="hytype">
                 select  * from  hytype
                 where  id=#{id}
      </select>
      


</mapper>


